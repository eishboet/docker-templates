<?xml version="1.0" encoding="utf-8"?>
<Container version="0.1">
  <Name>owncloud</Name>
  <Overview>
  </Overview>
  <Repository>eishboet/owncloud</Repository>
  <Registry>https://hub.docker.com/r/eishboet/owncloud</Registry>
  <Privileged>false</Privileged>
  <WebUI>http://[IP]:[PORT:80]/</WebUI>
  <Network>bridge</Network>
  <Icon>https://raw.githubusercontent.com/eishboet/docker-templates/master/icons/owncloud-icon.svg</Icon>
  <Category>Network:Web</Category>
  <Config Type="Variable" Name="PUID" Default="99" Description="PUID" Required="true">99</Config>
  <Config Type="Variable" Name="PGID" Default="100" Description="PGID" Required="true">100</Config>
  <Config Type="Port" Name="HTTP:// port" Target="80" Default="80" Mode="tcp" Description="container port" Display="always" Required="true">80</Config>
  <Config Type="Port" Name="HTTPS:// port" Target="443" Default="443" Mode="tcp" Description="container port" Display="always" Required="true">443</Config>
  <Config Type="Path" Name="/config/" Target="/config/" Default="/config/" Mode="rw" Description="nginx config folder" Required="true">/mnt/user/appdata/nginx/config/</Config>
  <Config Type="Path" Name="/var/www/owncloud/config/" Target="/var/www/owncloud/config/" Default="/var/www/owncloud/config/" Mode="rw" Description="owncloud config folder" Required="true">/mnt/user/appdata/owncloud/config/</Config>
  <Config Type="Path" Name="/var/www/" Target="/var/www/" Default="/var/www/" Mode="rw" Description="www folder" Required="true">/mnt/user/appdata/nginx/www/</Config>
</Container>
